pipeline {
 	agent any
 	stages{
 		stage('1-make a left'){
 			steps{
 			sh  'echo "walk.."'
 			}
 		}
 		stage('2-make a right'){
 			steps{
 				sh  'echo "walk.."'
 			}
 		}
 		stage('3-make another left'){
 			steps{
 				sh  'echo "walk.."'
 			}
 		}
 		stage('4-cross the street'){
 			steps{
 				sh  'echo "walk.."'
 			}
 		}
 	}  
}
